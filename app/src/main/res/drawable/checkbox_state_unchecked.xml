<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="20dp"
                android:height="20dp" />
            <corners
                android:radius="4dp" />
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
            <stroke
                android:width="1.5dp"
                android:color="@color/black" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <size
                android:width="20dp"
                android:height="20dp" />
            <corners
                android:radius="3dp" />
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
            <solid android:color="@color/figma_primary" />
        </shape>
    </item>

</layer-list>